{
  "extends": "@tsconfig/node22/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "target": "esnext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "esnext"
    ],
    "types": [
      "node",
      "@cloudflare/workers-types/2023-07-01"
    ],
    "esModuleInterop": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "declaration": true,
    "outDir": "src/d/",
    "sourceMap": true,
    // "inlineSourceMap": true,
  },
  "exclude": [
    "test",
    "src/d/**/*"
  ],
  "include": [
    "worker-configuration.d.ts",
    "src/**/*",
  ]
}
